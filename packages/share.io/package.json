{
    "name": "share.io",
    "version": "2.0.0",
    "description": "A simple file sharing made with socket.io",
    "type": "commonjs",
    "main": "./dist/index.js",
    "module": "./index.mjs",
    "typings": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./index.mjs"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "scripts": {
        "clean": "npx rimraf dist"
    },
    "dependencies": {
        "@discordjs/collection": "^1.3.0",
        "@types/node": "^18.11.17",
        "fallout-utility": "^1.5.21",
        "socket.io": "^4.5.4",
        "socket.io-client": "^4.5.4",
        "tiny-typed-emitter": "^2.1.0"
    }
}
